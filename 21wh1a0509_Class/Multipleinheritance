class One{  
  void display(){
    System.out.println("Main class");
  }  
}  
class Two extends One{  
  void display1(){
    System.out.println("Derived class");
  }  
}   
public class Multipleinheritance{  
  public static void main(String args[]){  
    Two t=new Two();    
    t.display1();  
    t.display();  
  }
}  
