class One{  
  void display1(){
    System.out.println("class one");
  }  
}  
class Two extends One{  
  void display2(){
  System.out.println("class two");
  }  
}  
class Three extends One{  
  void display3(){
    System.out.println("class three");
  }  
}  
class Hierachicaliheritance3{  
  public static void main(String args[]){  
    Three c=new Three();  
    c.display3();  
    c.display1();  
  }
} 
