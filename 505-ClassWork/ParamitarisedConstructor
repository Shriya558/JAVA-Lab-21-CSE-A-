import java.util.*;
import java.lang.*;
class ParamiterisedConstrucor{
    int age;
    float marks;
    ParamitarisedConstructor(int x,float y){
        age=x;
        marks=y;
    }
    
    
}

class Main{
    public static void main(String args[]){
        ParamitarisedConstructor s= new ParamitarisedConstructor(10,23.0f);
        System.out.println(s.age);
               System.out.println(s.marks);
    }
}
