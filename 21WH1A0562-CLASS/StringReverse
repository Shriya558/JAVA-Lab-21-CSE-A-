import java.util.Scanner;
class Main {
    public static void main(String[] args) {
        String name,rev;
        Scanner sc =  new Scanner(System.in);
        int t=sc.nextInt();
        int i;
        for(i=1;i<=t;i++){
            name = sc.next();
        StringBuilder sb = new StringBuilder(name);
        rev = sb.reverse().toString();
        System.out.println(rev);
        
        }
     }
}
    
