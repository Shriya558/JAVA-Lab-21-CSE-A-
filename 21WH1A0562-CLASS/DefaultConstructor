public class DefaultConstructor {
	int age;
	String name;
	DefaultConstructor(){
		age=20;
		name="abcd";
	}
	void display() {
		System.out.println(" name: "+name);
		System.out.print("age: "+age);
	}
}
class Main{
	public static void main(String [] args) {
		DefaultConstructor dc=new DefaultConstructor();
		dc.display();
		}
}
