package java_clss;
interface A{
	void method1();
}
interface B extends A{
	void method2();
}
class Child implements B{
	public void method1() {
		System.out.println("Method1 of interfaceA");
	}
	public void method2() {
		System.out.println("method2 of interfaceB");
	}
}
public class MultipleInheritance {
	public static void main(String [] args) {
	Child c=new Child();
	c.method1();
	c.method2();
}
}
