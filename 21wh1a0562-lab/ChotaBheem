package java_lab;
import java.util.*;
public class ChotaBheem {
	public static void main(String [] args) {
	int n,ele;
	System.out.print("enter size: ");
	Scanner sc=new Scanner(System.in);
	n=sc.nextInt();
	System.out.print("enter elements: ");
	Set<Integer>set =new HashSet<>();
	for(int i=0;i<n;i++){
		ele=sc.nextInt();
		set.add(ele);
	}
	int s=set.size();
	if(s>=n/2) {
		System.out.print("no of laddus : "+(n/2));
	}
	else
		System.out.print("no of laddus: "+s);		
	}
}
