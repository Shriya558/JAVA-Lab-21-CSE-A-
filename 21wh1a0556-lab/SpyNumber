import java.util.*;
class SpyNumber {
    public static void main(String[] args) {
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
 
    int sum=0,prod=1;
    while(n>0){
        sum+=n%10;
        prod*=n%10;
        n=n/10;
    }
    if(sum==prod)
    System.out.print("Spy Number\n");
    else
    System.out.print("Not a Spy Number\n");

    }
}
