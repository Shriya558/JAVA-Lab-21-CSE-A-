import java.util.*;
class Fibonacci {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int i = 1 , n1 = 0, n2 = 1;
        int n = sc.nextInt();
        if(n<=0)
        System.out.print("-1\n");
        else{
          while (i <= n) {
            System.out.print(n1+ " ");
            int n3 = n1 + n2;
            n1 = n2;
            n2 = n3;
            i++;
          }
      }
    }
}
